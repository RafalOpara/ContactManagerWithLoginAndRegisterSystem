@model CustomIdentity.Models.Contact

@{
    ViewData["Title"] = "Contact Details";
}

<h2>Contact Details</h2>

<div>
    <dl class="row">
        <dt class="col-sm-2">Name:</dt>
        <dd class="col-sm-10">@Model.Name</dd>

        <dt class="col-sm-2">Last Name:</dt>
        <dd class="col-sm-10">@Model.LastName</dd>

        <dt class="col-sm-2">Email:</dt>
        <dd class="col-sm-10">@Model.Email</dd>

        <dt class="col-sm-2">Password:</dt>
        <dd class="col-sm-10">@Model.Password</dd>

        <dt class="col-sm-2">Category:</dt>
        <dd class="col-sm-10">@Model.Category</dd>

        <dt class="col-sm-2">Phone Number:</dt>
        <dd class="col-sm-10">@Model.PhoneNumber</dd>
    </dl>
</div>

<div>
    <a asp-action="Edit" asp-route-contactId="@Model.Id" class="btn btn-primary">Edit</a>
</div>
